<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stone.fitnessworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitnessWorld">
        <activity android:name=".arm"></activity>
        <activity android:name=".ab" />
        <activity android:name=".yoga" />
        <activity android:name=".strength" />
        <activity android:name=".legs" />
        <activity android:name=".gear" />
        <activity android:name=".cardio" />
        <activity android:name=".butt" />
        <activity android:name=".Fitness.arm" />
        <activity android:name=".Fitness.ab" />
        <activity android:name=".Fitness" />
        <activity android:name=".Health" />
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>